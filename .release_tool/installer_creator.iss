#define MyAppPublisher "Cierra_Runis"
#define MyAppURL "https://github.com/Cierra-Runis"
#define MyAppExeName "danmaku.exe"
#define MyAppAssocName MyAppName + " File"
#define MyAppAssocExt ".danmaku"
